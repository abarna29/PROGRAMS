#include <stdio.h>
int main()
{
    int n,r,s=0;
    scanf("%d",&n);
    while(n>0)
    {
        r=n%10;
        s=s*10+r;
        n/=10;
    }
	printf("%d",s);
	return 0;
}

         ******************
         
         
#include <stdio.h>
#include<math.h>
int main()
{
    int n,r,s=0;
    scanf("%d",&n);
    int pow=1;
    while(n>pow)
    {
        r=(n/pow)%10;
        s=s*10+r;
        pow*=10;
    }
	printf("%d",s);
	return 0;
}
